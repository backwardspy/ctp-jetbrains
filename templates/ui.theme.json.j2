{% set editorScheme = "/themes/" ~ flavor.identifier ~ ".xml" -%}
{% set accentColor = mauve -%}
{% set secondaryAccentColor = yellow -%}
{% set primaryForeground = text -%}
{% set primaryBackground = base -%}
{% set secondaryBackground = surface0 -%}
{% set panelBackground = mantle -%}
{% set hoverBackground = surface0 -%}
{% set selectionBackground = surface0 -%}
{% set selectionInactiveBackground = base -%}
{% set borderColor = if(cond=flavor.dark, t=base, f=crust) -%}
{% set separatorColor = surface0 -%}
{% set fileColorBlend = if(cond=flavor.dark, t=0.15, f=0.1) -%}
{
  "name": "Catppuccin {{flavor.name}}",
  "dark": {{flavor.dark}},
  "author": "Catppuccin Org <releases@catppuccin.com>",
  "editorScheme": "{{editorScheme}}",
  "colors": {
    "rosewater": "#{{rosewater.hex}}",
    "flamingo": "#{{flamingo.hex}}",
    "mauve": "#{{mauve.hex}}",
    "pink": "#{{pink.hex}}",
    "maroon": "#{{maroon.hex}}",
    "red": "#{{red.hex}}",
    "peach": "#{{peach.hex}}",
    "yellow": "#{{yellow.hex}}",
    "green": "#{{green.hex}}",
    "teal": "#{{teal.hex}}",
    "blue": "#{{blue.hex}}",
    "sky": "#{{sky.hex}}",
    "sapphire": "#{{sapphire.hex}}",
    "lavender": "#{{lavender.hex}}",
    "crust": "#{{crust.hex}}",
    "mantle": "#{{mantle.hex}}",
    "base": "#{{base.hex}}",
    "surface0": "#{{surface0.hex}}",
    "surface1": "#{{surface1.hex}}",
    "surface2": "#{{surface2.hex}}",
    "overlay0": "#{{overlay0.hex}}",
    "overlay1": "#{{overlay1.hex}}",
    "overlay2": "#{{overlay2.hex}}",
    "text": "#{{text.hex}}",
    "subtext0": "#{{subtext0.hex}}",
    "subtext1": "#{{subtext1.hex}}",
    "accentColor": "#{{accentColor.hex}}",
    "secondaryAccentColor": "#{{secondaryAccentColor.hex}}",
    "primaryForeground": "#{{primaryForeground.hex}}",
    "primaryBackground": "#{{primaryBackground.hex}}",
    "secondaryBackground": "#{{secondaryBackground.hex}}",
    "panelBackground": "#{{panelBackground.hex}}",
    "hoverBackground": "#{{hoverBackground.hex}}",
    "selectionBackground": "#{{selectionBackground.hex}}",
    "selectionInactiveBackground": "#{{selectionInactiveBackground.hex}}",
    "borderColor": "#{{borderColor.hex}}",
    "separatorColor": "#{{separatorColor.hex}}"
  },
  "ui": {
    "*": {
      "arc": "7",
      "background": "primaryBackground",
      "borderColor": "borderColor",
      "disabledBackground": "primaryBackground",
      "foreground": "primaryForeground",
      "inactiveBackground": "primaryBackground",
      "selectionBackground": "selectionBackground",
      "selectionForeground": "primaryForeground",
      "selectionInactiveBackground": "selectionInactiveBackground",
      "selectionInactiveForeground": "primaryForeground",
      "separatorColor": "separatorColor"
    },
    "ActionButton": {
      "hoverBackground": "hoverBackground",
      "hoverBorderColor": "hoverBackground",
      "pressedBackground": "selectionBackground",
      "pressedBorderColor": "selectionBackground"
    },
    "Banner": {
      "errorBackground": "#{{mix(base=red, blend=base, amount=0.1) | get(key='hex')}}",
      "errorBorderColor": "#{{mix(base=red, blend=base, amount=0.15) | get(key='hex')}}",
      "errorForeground": "primaryForeground",
      "informativeBackground": "#{{mix(base=blue, blend=base, amount=0.1) | get(key='hex')}}",
      "informativeBorderColor": "#{{mix(base=blue, blend=base, amount=0.15) | get(key='hex')}}",
      "informativeForeground": "primaryForeground",
      "warningBackground": "#{{mix(base=peach, blend=base, amount=0.1) | get(key='hex')}}",
      "warningBorderColor": "#{{mix(base=peach, blend=base, amount=0.15) | get(key='hex')}}",
      "warningForeground": "primaryForeground"
    },
    "Bookmark": {
      "Mnemonic": {
        "iconBackground": "secondaryBackground",
        "iconBorderColor": "accentColor",
        "iconForeground": "primaryForeground"
      },
      "MnemonicAssigned": {
        "background": "secondaryBackground",
        "borderColor": "secondaryAccentColor",
        "foreground": "primaryForeground"
      },
      "MnemonicAvailable": {},
      "MnemonicCurrent": {
        "background": "selectionBackground",
        "borderColor": "accentColor",
        "foreground": "primaryForeground"
      },
      "iconBackground": "accentColor"
    },
    "Borders": {
      "ContrastBorderColor": "separatorColor",
      "color": "borderColor"
    },
    "Button": {
      "background": "panelBackground",
      "default": {
        "endBackground": "accentColor",
        "endBorderColor": "accentColor",
        "focusColor": "accentColor",
        "focusedBorderColor": "accentColor",
        "foreground": "crust",
        "startBackground": "accentColor",
        "startBorderColor": "accentColor"
      },
      "disabledBorderColor": "secondaryBackground",
      "endBackground": "secondaryBackground",
      "endBorderColor": "secondaryBackground",
      "focusedBorderColor": "secondaryBackground",
      "foreground": "primaryForeground",
      "startBackground": "secondaryBackground",
      "startBorderColor": "secondaryBackground"
    },
    "CheckBox": {
      "background": "panelBackground"
    },
    "ComboBox": {
      "ArrowButton": {
        "background": "secondaryBackground",
        "disabledIconColor": "lavender",
        "iconColor": "accentColor",
        "nonEditableBackground": "secondaryBackground"
      },
      "modifiedItemForeground": "accentColor",
      "nonEditableBackground": "secondaryBackground",
      "selectionBackground": "selectionBackground"
    },
    "CompletionPopup": {
      "matchForeground": "accentColor",
      "selectionBackground": "surface1",
      "selectionInactiveBackground": "surface1"
    },
    "Component": {
      "borderColor": "borderColor",
      "disabledBorderColor": "selectionBackground",
      "errorFocusColor": "red",
      "focusColor": "accentColor",
      "focusedBorderColor": "selectionBackground",
      "iconColor": "primaryForeground",
      "inactiveErrorFocusColor": "red",
      "inactiveWarningFocusColor": "yellow",
      "warningFocusColor": "yellow"
    },
    "Counter": {
      "background": "accentColor",
      "foreground": "primaryBackground"
    },
    "DefaultTabs": {
      "background": "panelBackground",
      "hoverBackground": "primaryBackground",
      "underlinedTabBackground": "panelBackground"
    },
    "DragAndDrop": {
      "borderColor": "selectionBackground"
    },
    "Editor": {
      "Toolbar": {
        "borderColor": "separatorColor"
      },
      "background": "primaryBackground",
      "shortcutForeground": "accentColor"
    },
    "EditorPane": {
      "splitBorder": "separatorColor"
    },
    "EditorTabs": {
      "background": "primaryBackground",
      "hoverBackground": "primaryBackground",
      "inactiveHoverBackground": "red",
      "inactiveUnderlineColor": "lavender",
      "underTabsBorderColor": "separatorColor",
      "underlineArc": 4,
      "underlineColor": "accentColor",
      "underlineHeight": 1,
      "underlinedTabBackground": "panelBackground",
      "unselectedBlend": 0.7
    },
    "FileColor": {
      "Blue": "#{{mix(base=blue, blend=base, amount=fileColorBlend) | get(key='hex')}}",
      "Green": "#{{mix(base=green, blend=base, amount=fileColorBlend) | get(key='hex')}}",
      "Orange": "#{{mix(base=peach, blend=base, amount=fileColorBlend) | get(key='hex')}}",
      "Rose": "#{{mix(base=red, blend=base, amount=fileColorBlend) | get(key='hex')}}",
      "Violet": "#{{mix(base=lavender, blend=base, amount=fileColorBlend) | get(key='hex')}}",
      "Yellow": "#{{mix(base=yellow, blend=base, amount=fileColorBlend) | get(key='hex')}}"
    },
    "Label": {
      "background": "panelBackground",
      "selectedForeground": "selectionBackground"
    },
    "Link": {
      "activeForeground": "accentColor",
      "hoverForeground": "accentColor",
      "pressedForeground": "secondaryAccentColor",
      "visitedForeground": "secondaryAccentColor"
    },
    "List": {
      "Button": {
        "leftRightInset": 8,
        "separatorInset": 4
      },
      "background": "panelBackground",
      "border": "4,0,4,0",
      "hoverBackground": "hoverBackground",
      "rowHeight": "24",
      "selectionInactiveBackground": "#{{if(cond=flavor.light, t=hoverBackground, f=base) | get(key='hex')}}"
    },
    "MainToolbar": {
      "Dropdown": {
        "hoverBackground": "hoverBackground",
        "maxWidth": 350,
        "pressedBackground": "hoverBackground"
      },
      "Icon": {
        "hoverBackground": "hoverBackground",
        "insets": "5,5,5,5",
        "pressedBackground": "hoverBackground"
      },
      "background": "crust",
      "inactiveBackground": "panelBackground"
    },
    "MemoryIndicator": {
      "allocatedBackground": "surface0",
      "usedBackground": "surface1"
    },
    "Notification": {
      "ToolWindow": {
        "errorBackground": "primaryBackground",
        "errorBorderColor": "red",
        "errorForeground": "primaryForeground",
        "informativeBackground": "primaryBackground",
        "informativeBorderColor": "secondaryAccentColor",
        "informativeForeground": "primaryForeground",
        "warningBackground": "primaryBackground",
        "warningBorderColor": "peach",
        "warningForeground": "primaryForeground"
      },
      "background": "panelBackground",
      "borderColor": "borderColor",
      "errorBackground": "primaryBackground",
      "errorBorderColor": "red",
      "errorForeground": "primaryForeground"
    },
    "NotificationsToolwindow": {
      "newNotification": {
        "background": "primaryBackground",
        "hoverBackground": "hoverBackground"
      }
    },
    "Panel": {
      "background": "panelBackground"
    },
    "PasswordField": {
      "background": "secondaryBackground"
    },
    "Plugins": {
      "Button": {
        "installBackground": "primaryBackground",
        "installBorderColor": "secondaryAccentColor",
        "installFillBackground": "secondaryAccentColor",
        "installFillForeground": "primaryBackground",
        "installFocusedBackground": "primaryBackground",
        "installForeground": "secondaryAccentColor",
        "updateBackground": "accentColor",
        "updateBorderColor": "accentColor",
        "updateForeground": "primaryBackground"
      },
      "SearchField": {
        "background": "panelBackground"
      },
      "SectionHeader": {
        "foreground": "primaryForeground"
      },
      "Tab": {
        "hoverBackground": "hoverBackground",
        "selectedBackground": "selectionBackground",
        "selectedForeground": "primaryForeground"
      },
      "borderColor": "borderColor",
      "hoverBackground": "hoverBackground",
      "lightSelectionBackground": "selectionBackground",
      "tagBackground": "panelBackground"
    },
    "Popup": {
      "Advertiser": {
        "background": "panelBackground",
        "fontSizeOffset": -1,
        "foreground": "text"
      },
      "Header": {
        "activeBackground": "panelBackground",
        "inactiveBackground": "panelBackground"
      },
      "borderColor": "separatorColor",
      "borderWidth": 1,
      "inactiveBorderColor": "separatorColor",
      "paintBorder": true
    },
    "ProgressBar": {
      "failedColor": "red",
      "failedEndColor": "maroon",
      "indeterminateEndColor": "secondaryAccentColor",
      "indeterminateStartColor": "accentColor",
      "passedColor": "green",
      "passedEndColor": "teal",
      "progressColor": "accentColor",
      "trackColor": "selectionBackground"
    },
    "RadioButton": {
      "background": "panelBackground"
    },
    "RunWidget": {
      "background": "secondaryBackground",
      "foreground": "text",
      "iconColor": "primaryForeground",
      "pressedBackground": "green",
      "separatorColor": "separatorColor",
      "stopBackground": "red"
    },
    "ScrollBar": {
      "Mac": {
        "Transparent": {
          "hoverThumbColor": "secondaryAccentColor"
        },
        "hoverThumbColor": "secondaryAccentColor"
      }
    },
    "SearchEverywhere": {
      "SearchField": {
        "background": "secondaryBackground"
      },
      "Tab": {
        "selectedBackground": "secondaryBackground",
        "selectedForeground": "primaryForeground"
      }
    },
    "SearchMatch": {
      "endBackground": "accentColor",
      "startBackground": "accentColor"
    },
    "Separator": {
      "separatorColor": "separatorColor"
    },
    "SidePanel": {
      "background": "panelBackground"
    },
    "Slider": {
      "background": "panelBackground"
    },
    "StatusBar": {
      "Breadcrumbs": {
        "chevronInset": 0
      },
      "background": "panelBackground",
      "borderColor": "borderColor",
      "hoverBackground": "hoverBackground"
    },
    "TabbedPane": {
      "background": "panelBackground",
      "contentAreaColor": "borderColor",
      "focusColor": "selectionBackground",
      "hoverColor": "hoverBackground",
      "tabHeight": 40,
      "tabSelectionArc": 4,
      "tabSelectionHeight": 1,
      "underlineColor": "accentColor"
    },
    "Table": {
      "gridColor": "hoverBackground",
      "hoverBackground": "selectionBackground",
      "lightSelectionBackground": "secondaryBackground"
    },
    "TableHeader": {
      "bottomSeparatorColor": "primaryBackground"
    },
    "TextArea": {
      "background": "panelBackground"
    },
    "TextField": {
      "background": "secondaryBackground",
      "selectionBackground": "#{{if(cond=flavor.light, t=mix(base=surface0, blend=base, amount=0.4), f=surface2) | get(key='hex')}}"
    },
    "ToggleButton": {
      "buttonColor": "primaryForeground",
      "offBackground": "selectionBackground",
      "offForeground": "hoverBackground",
      "onBackground": "green",
      "onForeground": "hoverBackground"
    },
    "ToolBar": {
      "background": "panelBackground",
      "borderHandleColor": "secondaryAccentColor"
    },
    "ToolWindow": {
      "Button": {
        "hoverBackground": "hoverBackground",
        "selectedBackground": "hoverBackground",
        "selectedForeground": "primaryForeground"
      },
      "Header": {
        "background": "panelBackground",
        "borderColor": "borderColor",
        "inactiveBackground": "panelBackground"
      },
      "HeaderTab": {
        "hoverBackground": "surface0",
        "inactiveUnderlineColor": "text",
        "underlineColor": "accentColor",
        "underlineHeight": 1
      },
      "background": "panelBackground"
    },
    "Tree": {
      "background": "panelBackground",
      "border": "4,12,4,12",
      "foreground": "primaryForeground",
      "hoverBackground": "hoverBackground",
      "modifiedItemForeground": "accentColor",
      "rowHeight": 24,
      "selectionBackground": "selectionBackground",
      "selectionInactiveBackground": "selectionBackground"
    },
    "UiDesigner": {
      "Panel.background": "panelBackground",
      "Preview.background": "panelBackground"
    },
    "ValidationTooltip": {
      "errorBackground": "panelBackground",
      "errorBorderColor": "red",
      "warningBackground": "panelBackground",
      "warningBorderColor": "secondaryAccentColor"
    },
    "VersionControl": {
      "FileHistory": {
        "Commit": {
          "selectedBranchBackground": "secondaryBackground"
        }
      },
      "GitLog": {
        "headIconColor": "secondaryAccentColor",
        "localBranchIconColor": "green",
        "otherIconColor": "teal",
        "remoteBranchIconColor": "secondaryAccentColor",
        "tagIconColor": "accentColor"
      },
      "Log": {
        "Commit": {
          "currentBranchBackground": "panelBackground",
          "hoveredBackground": "hoverBackground",
          "selectionInactiveBackground": "selectionInactiveBackground"
        }
      },
      "RefLabel": {
        "foreground": "primaryForeground"
      }
    },
    "WelcomeScreen": {
      "Projects": {
        "actions": {
          "background": "hoverBackground"
        },
        "background": "hoverBackground",
        "selectionBackground": "secondaryBackground",
        "selectionInactiveBackground": "secondaryBackground"
      },
      "SidePanel": {
        "background": "panelBackground"
      },
      "separatorColor": "separatorColor"
    }
  },
  "icons": {
    "ColorPalette": {
      "Actions.Blue": "#{{blue.hex}}",
      "Actions.Green": "#{{green.hex}}",
      "Actions.Grey": "#{{overlay0.hex}}",
      "Actions.GreyInline": "#{{blue.hex}}",
      "Actions.GreyInline.Dark": "#{{blue.hex}}",
      "Actions.Red": "#{{red.hex}}",
      "Actions.Yellow": "#{{yellow.hex}}",
      "Objects.BlackText": "#{{surface0.hex}}",
      "Objects.Blue": "#{{blue.hex}}",
      "Objects.Green": "#{{green.hex}}",
      "Objects.Grey": "#{{overlay0.hex}}",
      "Objects.Pink": "#{{pink.hex}}",
      "Objects.Purple": "#{{mauve.hex}}",
      "Objects.Red": "#{{red.hex}}",
      "Objects.RedStatus": "#{{red.hex}}",
      "Objects.Yellow": "#{{yellow.hex}}",
      "Objects.YellowDark": "#{{flamingo.hex}}",
      "Checkbox.Background.Default": "#{{surface0.hex}}",
      "Checkbox.Background.Default.Dark": "#{{surface0.hex}}",
      "Checkbox.Background.Disabled": "#{{surface0.hex}}",
      "Checkbox.Background.Disabled.Dark": "#{{surface0.hex}}",
      "Checkbox.Background.Selected": "#{{overlay0.hex}}",
      "Checkbox.Background.Selected.Dark": "#{{surface1.hex}}",
      "Checkbox.Border.Default": "#{{surface0.hex}}",
      "Checkbox.Border.Disabled": "#{{surface0.hex}}",
      "Checkbox.Border.Selected": "#{{surface0.hex}}",
      "Checkbox.Foreground.Disabled": "#{{overlay0.hex}}",
      "Checkbox.Foreground.Disabled.Dark": "#{{overlay0.hex}}",
      "Checkbox.Foreground.Selected": "#{{accentColor.hex}}",
      "Checkbox.Foreground.Selected.Dark": "#{{accentColor.hex}}",
      "Tree.iconColor": "#{{text.hex}}"
    }
  }
}